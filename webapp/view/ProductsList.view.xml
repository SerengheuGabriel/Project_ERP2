<mvc:View
	controllerName="sap.ui.demo.walkthrough.controller.SupplierList"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<Page>
		<headerContent>
			<Title text="{i18n>appTitle}"/>
		</headerContent>
		<content>
			<Table items="{
				path: '/Products'
			}">
			<columns>
				<Column />
				<Column />
			</columns>
			<items>
				<ColumnListItem>
					<Label text="ok" />
					<Label text="{productName}" />
					<Label text="ok2" />
					<Label text="{SupplierID}" />
				</ColumnListItem>
			</items>
		</content>
		<footer>
				<OverflowToolbar>
				<ToolbarSpacer/>
				<Title text="{i18n>appDevelopers}"/>
			</OverflowToolbar>
		</footer>
	</Page>
</mvc:View>