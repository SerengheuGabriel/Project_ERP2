<mvc:View
		controllerName="sap.ui.demo.walkthrough.controller.App"
		xmlns="sap.m"
		xmlns:mvc="sap.ui.core.mvc">
	<Page>
		<headerContent>
			<Title text="{i18n>appTitle}"/>
		</headerContent>
		<content>
			<mvc:XMLView viewName="sap.ui.demo.walkthrough.view.SuppliersList"/>
		</content>
		<footer>
				<OverflowToolbar>
				<ToolbarSpacer/>
				<Title text="{i18n>appDevelopers}"/>
			</OverflowToolbar>
		</footer>
	</Page>
</mvc:View>